{
    "builder": "custom",
    "default": "build",
    "kill": "binarysupport",
    "dependencies_host": [
        "gcc-9-aarch64-linux-gnu",
        "gcc-9-arm-linux-gnueabihf",
        "g++-9-aarch64-linux-gnu",
        "g++-9-arm-linux-gnueabihf",
        "debootstrap",
        "fakeroot",
        "fakechroot",
        "qemu-user-static",
        "libc6-dev-armhf-cross",
        "libx11-dev",
        "patchelf",
        "apt-rdepends"
    ],
    "dependencies_target": [
        "libglvnd-dev",
        "libfuse2",
        "libwayland-dev",
        "libxmp-dev",
        "fuse",
        "wayland-protocols",
        "libappindicator1",
        "libsdl2-gfx-1.0-0",
        "libsdl2-net-2.0-0",
        "libsdl2-ttf-2.0-0",
        "libsdl2-image-2.0-0",
        "libsdl2-mixer-2.0-0",
        "libsdl2-dev"
    ],
    "install_lib": [
        "/lib/${ARCH_TRIPLET}/libfuse.so*",
        "/lib/${ARCH_TRIPLET}/libulockmgr.so*",
        "/lib/${ARCH_TRIPLET}/libappindicator.so*",
        "/lib/${ARCH_TRIPLET}/libSDL2*.so*",
        "/lib/libopusfile.so*",
        "/lib/libopusurl.so*"
    ],
    "install_bin": [
        "/usr/bin/fusermount"
    ],
    "build": "${ROOT}/build.sh -c",
    "make_jobs": 6,
    "qt_version": "5.12",
    "framework": "ubuntu-sdk-20.04"
}
